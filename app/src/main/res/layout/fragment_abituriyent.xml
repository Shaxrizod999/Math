<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F5F5"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.AbituriyentFragment">

    <androidx.cardview.widget.CardView

        android:id="@+id/dtmtestlari"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintWidth_percent="0.8"
        app:layout_constraintVertical_bias="0.2"
        android:layout_margin="10dp"
        app:cardCornerRadius="10dp"
        android:elevation="15dp"
        android:layout_width="0dp"
        android:layout_height="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:scaleX="1"
            android:layout_height="match_parent">

            <ImageView

                android:id="@+id/images"
                android:src="@color/white"
                android:scaleX="5"
                android:scaleY="2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView

                android:textSize="28sp"
                android:textColor="@color/black"
                android:text="DTM testlari"
                android:fontFamily="sans-serif"
                app:layout_constraintTop_toTopOf="@id/images"
                app:layout_constraintBottom_toBottomOf="@id/images"
                app:layout_constraintRight_toRightOf="@id/images"
                app:layout_constraintLeft_toLeftOf="@id/images"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView

        android:id="@+id/masalalartoplami"
        app:layout_constraintTop_toBottomOf="@id/dtmtestlari"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintWidth_percent="0.8"
        app:layout_constraintVertical_bias="0"
        android:layout_margin="20dp"
        app:cardCornerRadius="10dp"
        android:elevation="15dp"
        android:layout_width="0dp"
        android:layout_height="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:scaleX="1"
            android:layout_height="match_parent">

            <ImageView

                android:id="@+id/imag"
                android:src="@color/white"
                android:scaleX="5"
                android:scaleY="2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView

                android:textSize="28sp"
                android:textColor="@color/black"
                android:text="Masalalar to'plami"
                android:fontFamily="sans-serif"
                app:layout_constraintTop_toTopOf="@id/imag"
                app:layout_constraintBottom_toBottomOf="@id/imag"
                app:layout_constraintRight_toRightOf="@id/imag"
                app:layout_constraintLeft_toLeftOf="@id/imag"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView

        android:id="@+id/abituriyent"
        app:layout_constraintTop_toBottomOf="@id/masalalartoplami"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintWidth_percent="0.8"
        app:layout_constraintVertical_bias="0"
        app:cardCornerRadius="10dp"
        android:layout_margin="20dp"
        android:elevation="15dp"
        android:layout_width="0dp"
        android:layout_height="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:scaleX="1"
            android:layout_height="match_parent">

            <ImageView

                android:id="@+id/rasm"
                android:src="@color/white"
                android:scaleX="5"
                android:scaleY="2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView

                android:textSize="28sp"
                android:textColor="@color/black"
                android:text="Abituriyent"
                android:fontFamily="sans-serif"
                app:layout_constraintTop_toTopOf="@id/rasm"
                app:layout_constraintBottom_toBottomOf="@id/rasm"
                app:layout_constraintRight_toRightOf="@id/rasm"
                app:layout_constraintLeft_toLeftOf="@id/rasm"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView

        android:id="@+id/qollanma"
        app:layout_constraintTop_toBottomOf="@id/abituriyent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintWidth_percent="0.8"
        app:layout_constraintVertical_bias="0"
        android:layout_margin="20dp"
        app:cardCornerRadius="10dp"
        android:elevation="15dp"
        android:layout_width="0dp"
        android:layout_height="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout

            android:layout_width="match_parent"
            android:scaleX="1"
            android:layout_height="match_parent">

            <ImageView

                android:id="@+id/ras"
                android:src="@color/white"
                android:scaleX="5"
                android:scaleY="2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView

                android:textSize="28sp"
                android:textColor="@color/black"
                android:text="Qo'llanmalar"
                android:fontFamily="sans-serif"
                app:layout_constraintTop_toTopOf="@id/ras"
                app:layout_constraintBottom_toBottomOf="@id/ras"
                app:layout_constraintRight_toRightOf="@id/ras"
                app:layout_constraintLeft_toLeftOf="@id/ras"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>

