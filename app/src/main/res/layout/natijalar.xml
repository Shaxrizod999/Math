<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/back_style_natija"
    android:layout_height="wrap_content">



    <TextView

        android:textColor="@color/black"
        android:textSize="25sp"
        android:layout_margin="20dp"
        android:textStyle="bold"
        android:text = "Test natijalari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView

        android:id="@+id/jamisavollarsoni"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:layout_margin="10dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView

        android:id="@+id/belgilanmaganlarsoni"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:layout_margin="10dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <TextView

        android:id="@+id/togrijavolarsoni"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:layout_margin="10dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <TextView

        android:id="@+id/testdavomiyligi"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:layout_margin="10dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <Button

        android:id="@+id/back"
        android:text="Variantlar bo'limiga qaytish"
        android:textAllCaps="false"
        android:backgroundTint="@color/teal_700"
        android:layout_margin="5dp"
        android:layout_width="180dp"
        android:layout_height="wrap_content"/>
</LinearLayout>